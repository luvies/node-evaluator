{
  "name": "@luvies/evaluator",
  "version": "0.1.0",
  "description": "A simple, secure expression evaluator",
  "main": "dist/index.js",
  "repository": "https://github.com/luvies/node-evaluator",
  "author": "luvies",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest --expand",
    "test:ci": "jest --ci --expand",
    "test:watch": "jest --watch",
    "lint": "eslint --ext .js,.ts . && prettier --check '**/*.{ts,js,json,md,tsx,jsx,html,scss,css,yaml,yml,vue}'",
    "fix": "eslint --ext .js,.ts --fix . && prettier --check --write '**/*.{ts,js,json,md,tsx,jsx,html,scss,css,yaml,yml,vue}'",
    "postinstall": "rm -rf ./node_modules/jsep/typings/*"
  },
  "devDependencies": {
    "@luvies/config": "^1.3.4",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.4.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "jsep": "^0.3.4",
    "tslib": "^1.10.0"
  }
}
