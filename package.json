{
  "name": "@luvies/evaluator",
  "version": "0.2.1",
  "description": "A simple, secure expression evaluator",
  "main": "dist/index.js",
  "repository": "https://github.com/luvies/node-evaluator",
  "author": "luvies",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "yarn.lock"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist/* coverage *.tsbuildinfo",
    "test": "jest --expand",
    "test:ci": "jest --ci --expand",
    "test:watch": "jest --watch",
    "test:coverage": "cat ./coverage/lcov.info | coveralls",
    "lint": "./node_modules/@luvies/config/scripts/lint.sh",
    "fix": "./node_modules/@luvies/config/scripts/lint.sh fix",
    "postinstall": "rm -rf ./node_modules/jsep/typings/*"
  },
  "devDependencies": {
    "@luvies/config": "^2.0.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "coveralls": "^3.0.6",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "jsep": "^0.3.4",
    "tslib": "^1.10.0"
  }
}
