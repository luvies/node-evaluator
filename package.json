{
  "name": "@luvies/evaluator",
  "version": "0.2.0",
  "description": "A simple, secure expression evaluator",
  "main": "dist/index.js",
  "repository": "https://github.com/luvies/node-evaluator",
  "author": "luvies",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "yarn.lock"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist/* coverage *.tsbuildinfo",
    "test": "jest --expand",
    "test:ci": "jest --ci --expand",
    "test:watch": "jest --watch",
    "test:coverage": "cat ./coverage/lcov.info | coveralls",
    "lint": "./node_modules/@luvies/config/scripts/lint.sh",
    "fix": "./node_modules/@luvies/config/scripts/lint.sh fix",
    "postinstall": "rm -rf ./node_modules/jsep/typings/*"
  },
  "devDependencies": {
    "@luvies/config": "^1.4.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.4.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "jsep": "^0.3.4",
    "tslib": "^1.10.0"
  }
}
